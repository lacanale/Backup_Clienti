PROC POS_SHIFT(REAL ANGOLO,REAL POS_Y,REAL POS_Z)
;R910=NUOVA Y
;R911=NUOVA Z
POS_Y=R900
POS_Z=R901
;ANGOLO=R920
;R910=POS_Z*SIN(ANGOLO)+POS_Y*COS(ANGOLO)
;R911=POS_Z*COS(ANGOLO)-POS_Y*SIN(ANGOLO)

R910=POS_Z*SIN(R920)+POS_Y*COS(R920)
R911=POS_Z*COS(R920)-POS_Y*SIN(R920)

MSG("Y="<<R910<<"  Z="<<R911<<"  ANGOLO="<<R920)
TRANS Y=R102+R910 Z=R103+R911
M17

